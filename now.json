{
  "version": 2,
  "name": "meteorite",
  "alias": "meteorite.dev",
  "builds": [{
    "src": "package.json",
    "use": "@now/static-build",
    "config": {"distDir": "build"}
  }],
  "routes": [
    {"src": "^/static/(.*)", "dest": "/static/$1"},
    {"src": "^/favicon.ico", "dest": "/iconCircle.png"},
    {"src": "^/iconCircle.png", "dest": "/iconCircle.png"},
    {"src": "^/manifest.json", "dest": "/manifest.json"},
    {"src": "^/(.*)", "dest": "/index.html"}
  ],
  "github": {
    "enabled": false
  }
}
